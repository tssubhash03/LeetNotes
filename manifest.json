{
  "manifest_version": 3,
  "name": "LeetCode Problem Extractor",
  "version": "1.0",
  "permissions": ["scripting", "tabs", "storage","runtime"],
  "host_permissions": ["https://leetcode.com/*"],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
